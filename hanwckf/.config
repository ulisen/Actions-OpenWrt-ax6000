CONFIG_TARGET_mediatek_mt7986_DEVICE_xiaomi_redmi-router-ax6000=y

## 主题
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-app-argon-config=y
CONFIG_PACKAGE_luci-compat=y

## -----------自己加的第三方插件源 START-----------
## LuCI->Applications
CONFIG_PACKAGE_luci-app-adguardhome=y 
CONFIG_PACKAGE_luci-app-alist=y                   #Alist
CONFIG_PACKAGE_luci-app-wrtbwmon=y                #Wrtbwmon
#CONFIG_PACKAGE_luci-app-oaf=y                    #OpenAppFilter过滤器
CONFIG_PACKAGE_luci-app-openclash=y               #OpenClash
CONFIG_PACKAGE_luci-app-ddns-go=y                 #动态DNS
CONFIG_PACKAGE_luci-app-ddns=n                    #动态DNS
CONFIG_PACKAGE_ddns-scripts=n
CONFIG_PACKAGE_ddns-scripts-aliyun=n
CONFIG_PACKAGE_luci-app-adbyby-plus=y 

## -----------自己加的第三方插件源  END------------

## LuCI->Applications
